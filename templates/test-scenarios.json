{
  "workflow_test_scenarios": {
    "scenario_1_technical_implementation": {
      "name": "E-commerce Platform API Development",
      "focus_area": "technical",
      "complexity": "high",
      "duration": "4_weeks",
      "test_parameters": {
        "team_size": 6,
        "parallel_agents": ["architect", "backend_dev", "frontend_dev", "devops", "tester", "security_specialist"],
        "external_dependencies": ["payment_gateway", "inventory_system", "shipping_api"],
        "constraints": ["PCI_compliance", "99.9_uptime", "sub_200ms_response"]
      },
      "expected_outcomes": {
        "deliverables": [
          "API specification documentation",
          "Microservices architecture",
          "Database schema and migrations",
          "Authentication and authorization system",
          "Payment processing integration",
          "Order management system",
          "Comprehensive test suite",
          "Deployment pipeline"
        ],
        "success_metrics": {
          "code_coverage": ">=90%",
          "api_response_time": "<=200ms",
          "uptime": ">=99.9%",
          "security_score": "A_grade"
        },
        "checkpoint_validations": [
          "Day 3: Architecture approved by tech lead",
          "Day 7: Core API endpoints functional",
          "Day 14: Payment integration working",
          "Day 21: Load testing passed",
          "Day 28: Production deployment complete"
        ]
      },
      "test_variations": [
        {
          "variation": "resource_constraint",
          "change": "reduce_team_size_to_4",
          "expected_impact": "extend_timeline_by_1_week"
        },
        {
          "variation": "scope_expansion",
          "change": "add_mobile_app_api",
          "expected_impact": "require_additional_frontend_specialist"
        },
        {
          "variation": "technology_change",
          "change": "switch_from_rest_to_graphql",
          "expected_impact": "architecture_redesign_needed"
        }
      ]
    },
